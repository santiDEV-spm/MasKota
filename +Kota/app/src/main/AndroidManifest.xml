<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="coursera.santi.kota">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitis.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitis.Main2Activity"
            android:parentActivityName=".activitis.MainActivity"
            >
            <!-- Soporte para versiones 4.0 o anteriores-->
           <meta-data
               android:name="android.support.PARENT_ACTIVITY"
               android:value=".activitis.MainActivity">
           </meta-data>

        </activity>
    </application>

</manifest>